{
  "manifest_version": 3,
  "name": "Show a Random Trello Card",
  "version": "1.0",
  "description": "Displays a random card from a specified Trello board in new tabs",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://api.trello.com/*"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "48": "icons/icon.svg",
    "128": "icons/icon.svg"
  }
}
